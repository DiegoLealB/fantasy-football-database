<div class="greeting">
  <h1>Search results</h1>
  <form action="/dash" method="GET">
    <button class="btn create-team-btn">Go back to your dashboard</button>
  </form>
</div>

<div class="search-list-container">
  <ul>
    {{#each results}}
      <h3><li>
        <a href="/player/{{this.PlayerID}}">{{this.FirstName}} {{this.LastName}}</a>
      </li></h3>
    {{/each}}
  </ul>
</div>